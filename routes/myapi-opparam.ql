-- Finds items on eBay based upon a keyword query and returns details for matching items.
return select *
from finditems where affiliate.customId = '{affiliatecustomId}' and affiliate.geoTargeting = '{affiliategeoTargeting}' and affiliate.networkId = '{affiliatenetworkId}' and affiliate.trackingId = '{affiliatetrackingId}' and buyerPostalCode = '{buyerPostalCode}' and paginationInput.entriesPerPage = '{paginationInputentriesPerPage}' and paginationInput.pageNumber = '{paginationInputpageNumber}' and sortOrder = '{sortOrder}' and aspectFilter.aspectName = '{aspectFilteraspectName}' and aspectFilter.aspectValueName = '{aspectFilteraspectValueName}' and domainFilter.domainName = '{domainFilterdomainName}' and itemFilter.name = '{itemFiltername}' and itemFilter.paramName = '{itemFilterparamName}' and itemFilter.paramValue = '{itemFilterparamValue}' and itemFilter.value = '{itemFiltervalue}' and keywords = '{keywords}' and outputSelector = '{outputSelector}'
via route '/ebay/finding/keywords?affiliatecustomid={affiliatecustomId}&affiliategeotargeting={affiliategeoTargeting}&affiliatenetworkid={affiliatenetworkId}&affiliatetrackingid={affiliatetrackingId}&buyerpostalcode={buyerPostalCode}&paginationinputentriesperpage={paginationInputentriesPerPage}&paginationinputpagenumber={paginationInputpageNumber}&sortorder={sortOrder}&aspectfilteraspectname={aspectFilteraspectName}&aspectfilteraspectvaluename={aspectFilteraspectValueName}&domainfilterdomainname={domainFilterdomainName}&itemfiltername={itemFiltername}&itemfilterparamname={itemFilterparamName}&itemfilterparamvalue={itemFilterparamValue}&itemfiltervalue={itemFiltervalue}&keywords={^keywords}&outputselector={outputSelector}'
with optional params
using defaults limit = 1, keywords = 'iphone'
using method get;

